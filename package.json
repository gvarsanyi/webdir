{
  "name": "webdir",
  "version": "2.0.0",
  "description": "static http server for multiple interfaces/ports",
  "main": "js/server.js",
  "bin": {
    "webdir": "js/webdir-cli.js"
  },
  "scripts": {
    "dev": "bun src/webdir-cli.ts",
    "dist": "tsc && echo \"#!/usr/bin/env node\" | cat - js/webdir.js > /tmp/out && mv /tmp/out js/webdir.js && chmod a+x js/webdir.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/gvarsanyi/webdir.git"
  },
  "keywords": [
    "static",
    "content",
    "web",
    "dir",
    "directory",
    "folder",
    "http-server",
    "http",
    "server"
  ],
  "author": "Greg Varsanyi",
  "license": "ISC",
  "dependencies": {
    "mime": "^4.0.1"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.2",
    "@types/node": "20.7.0",
    "@typescript-eslint/parser": "^5.62.0",
    "bun": "^1.0.26",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^51.0.1",
    "prettier": "^3.2.5",
    "typescript": "5.2.2"
  }
}
