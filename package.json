{
  "name": "webdir",
  "version": "0.9.6",
  "description": "static http server for multiple interfaces/ports",
  "main": "js/server.js",
  "bin": {
    "webdir": "js/webdir.js"
  },
  "scripts": {
    "dist": "tsc && echo \"#!/usr/bin/env node\" | cat - js/webdir.js > /tmp/out && mv /tmp/out js/webdir.js && chmod a+x js/webdir.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/gvarsanyi/webdir.git"
  },
  "keywords": [
    "static",
    "content",
    "web",
    "dir",
    "directory",
    "folder",
    "http-server",
    "http",
    "server"
  ],
  "author": "Greg Varsanyi",
  "license": "ISC",
  "dependencies": {
    "express": "4.18.2",
    "minimist": "1.2.8",
    "serve-index": "1.9.1"
  },
  "devDependencies": {
    "@types/express": "4.17.18",
    "@types/express-serve-static-core": "4.17.37",
    "@types/minimist": "1.2.3",
    "@types/node": "20.7.0",
    "@types/serve-index": "1.9.2",
    "@types/serve-static": "1.15.3",
    "typescript": "5.2.2"
  }
}
